#!/bin/sh

if [ $SAKILA_DB ]; then
   if [ -d /tmp/12345 ]; then
      /tmp/12345/use < $SAKILA_DB/sakila-schema.sql
      /tmp/12345/use < $SAKILA_DB/sakila-data.sql
      echo "Done loading the sakila database."
   else
      echo "The sandbox master does not exist at /tmp/12345/."
   fi
else
   echo "The SAKILA_DB environment variable is not set."
fi
