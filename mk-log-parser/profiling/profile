#!/bin/sh

REV=`grep 'SVN_REV.*Revision' ../mk-log-parser  | awk '{print $6}'`;
/usr/bin/time perl -d:FastProf ../mk-log-parser barontest.log > /dev/null 2>times
echo "" >> profiling-results.txt
echo "Revision $REV" >> profiling-results.txt
cat times >> profiling-results.txt
fprofpp -t 30 >> profiling-results.txt
svn ci -m "Profiling results as of revision $REV" profiling-results.txt
